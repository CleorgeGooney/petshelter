{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth0": {
    "Domain": "{domain}",
    "ApiIdentifier": "{url}",
    "ManagementDomain": "{managementDomain}",
    "ManagementToken": "{managementToken}"
  },
  "Database": {
    "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=petshelterdb;Trusted_Connection=True;ConnectRetryCount=0"
  },
  "PetApi": {
    "Dog": {
      "Audience": "https://api.thedogapi.com/v1/",
      "Token": "{token}"
    },
    "Cat": {
      "Audience": "https://api.thecatapi.com/v1/",
      "Token": "{token}"
    }
  }
}
